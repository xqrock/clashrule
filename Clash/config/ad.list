payload:
  - '+.ads.youtube.com'
  - '+.fcmatch.youtube.com'
  - '+.s2.youtube.com'
  - '+.wyoutube.fr'
  - '+.youtube.local'

rule-providers:
  adblock:
    type: http
    behavior: domain
    format: yaml
    url: https://raw.githubusercontent.com/REIJI007/AdBlock_Rule_For_Clash/main/adblock_reject.yaml
    path: ./ruleset/adblock_reject.yaml
    interval: 120
    
rules:
  - RULE-SET,adblock,REJECT

